<?xml version="1.0" encoding="utf-8" ?>
<views:BasePage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Sample.Maui.Views"
    xmlns:viewModels="clr-namespace:Sample.Maui.ViewModels"
    x:Class="Sample.Maui.Views.HomePage"
    x:DataType="viewModels:HomeVm"
    Title="Samples">

    <StackLayout>
        <Button Text="Pick" Command="{Binding NavigateToPickCommand, Mode=OneTime}"/>
        <Button Text="Save" Command="{Binding NavigateToSaveCommand, Mode=OneTime}"/>
    </StackLayout>
</views:BasePage>
